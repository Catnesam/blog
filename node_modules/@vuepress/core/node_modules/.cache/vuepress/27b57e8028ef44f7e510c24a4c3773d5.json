A;AAUA;AAbA;AAfA,CAAA","sourcesContent":["<template>\n  <div class=\"\">\n    <Common\n      :sidebar=\"false\"\n      :isComment=\"false\"\n    >\n      <section class=\"project\">\n        <h1 class=\"project-title\">My Project</h1>\n        <h4 class=\"project-subtitle\">如果你觉得下面的项目可能还不错, 别忘了给个 start 吧！</h4>\n        <section\n          class=\"project-loading\"\n          v-if=\"loading\"\n        >\n          <img src=\"./img/loading.svg\">\n          <span>Loading...</span>\n        </section>\n        <section\n          class=\"project-container\"\n          v-else\n        >\n          <article\n            v-for=\"(item, index) in projects\"\n            :key=\"index\"\n          >\n            <project-item :options=\"item\"></project-item>\n          </article>\n        </section>\n      </section>\n    </Common>\n  </div>\n</template>\n\n<script>\n  import Common from \"@theme/components/Common.vue\";\n  import { httpGet } from \"./utils/fetch.js\";\n  import ProjectItem from \"./ProjectItem\";\n  import { filter, orderBy } from \"lodash\";\n  export default {\n    name: \"Projects\",\n    components: {\n      Common,\n      ProjectItem\n    },\n    data() {\n      return {\n        projects: [],\n        loading: true\n      };\n    },\n    mounted() {\n      this.getProjects();\n    },\n    methods: {\n      getProjects() {\n        this.loading = true;\n        httpGet(\"https://api.github.com/users/zpj80231/repos\").then(res => {\n          this.loading = false;\n          // 过滤掉私有的项目, 暂不过滤掉 fork 项目 && !item.fork\n          const projects = filter(res, item => !item.private);\n          this.projects = orderBy(\n            projects,\n            [\"stargazers_count\", \"forks_count\", \"name\", \"created_at\"],\n            [\"desc\", \"desc\", \"asc\", \"desc\"]\n          );\n        });\n      }\n    }\n  };\n</script>\n\n<style lang=\"css\" scoped>\n  .project {\n    max-width: 1024px;\n    min-height: 300px;\n    margin: 4rem auto;\n    margin-top: 6rem;\n  }\n  .project-title,\n  .project-subtitle {\n    text-align: center;\n  }\n  .project-loading {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n  .project-loading img {\n    position: relative;\n  }\n  .project-loading span {\n    position: absolute;\n  }\n  .project-container {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-around;\n  }\n</style>"],"sourceRoot":"docs/.vuepress/components"}]}