,CADiE,CAEjE;;AACA,YAAMD,QAAQ,GAAGL,MAAM,CAACW,GAAD,EAAM,UAAAC,IAAI;AAAA,iBAAI,CAACA,IAAI,WAAT;AAAA,SAAV,CAAvB;AACA,QAAA,KAAI,CAACP,QAAL,GAAgBJ,OAAO,CACrBI,QADqB,EAErB,CAAC,kBAAD,EAAqB,aAArB,EAAoC,MAApC,EAA4C,YAA5C,CAFqB,EAGrB,CAAC,MAAD,EAAS,MAAT,EAAiB,KAAjB,EAAwB,MAAxB,CAHqB,CAAvB;AAKD,OATD;AAUD;AAbM;AAfI,CAAf","sourcesContent":["//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n\nimport Common from \"@theme/components/Common.vue\";\nimport { httpGet } from \"./utils/fetch.js\";\nimport ProjectItem from \"./ProjectItem\";\nimport { filter, orderBy } from \"lodash\";\nexport default {\n  name: \"Projects\",\n  components: {\n    Common,\n    ProjectItem\n  },\n  data() {\n    return {\n      projects: [],\n      loading: true\n    };\n  },\n  mounted() {\n    this.getProjects();\n  },\n  methods: {\n    getProjects() {\n      this.loading = true;\n      httpGet(\"https://api.github.com/users/catnesam/repos\").then(res => {\n        this.loading = false;\n        // 过滤掉私有的项目, 暂不过滤掉 fork 项目 && !item.fork\n        const projects = filter(res, item => !item.private);\n        this.projects = orderBy(\n          projects,\n          [\"stargazers_count\", \"forks_count\", \"name\", \"created_at\"],\n          [\"desc\", \"desc\", \"asc\", \"desc\"]\n        );\n      });\n    }\n  }\n};\n"]}]}